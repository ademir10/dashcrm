<p id="notice"><%= notice %></p>

<h1>Agenda de compromissos <%= image_tag 'calendar.png' %></h1>

<%= month_calendar events: @meetings do |date, meetings| %>
  <%= date %>

  <% meetings.each do |meeting| %>
    <div>
      <%= link_to meeting.name, meeting.research_path %>
    </div>
  <% end %>
<% end %>

<br>
