<p id="notice"><%= notice %></p>

<h1>Listing Results</h1>

<table>
  <thead>
    <tr>
      <th>A1</th>
      <th>A2</th>
      <th>A3</th>
      <th>A4</th>
      <th>A5</th>
      <th>A6</th>
      <th>A7</th>
      <th>A8</th>
      <th>A9</th>
      <th>A10</th>
      <th>A11</th>
      <th>A12</th>
      <th>A13</th>
      <th>A14</th>
      <th>A15</th>
      <th>Research</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td><%= result.a1 %></td>
        <td><%= result.a2 %></td>
        <td><%= result.a3 %></td>
        <td><%= result.a4 %></td>
        <td><%= result.a5 %></td>
        <td><%= result.a6 %></td>
        <td><%= result.a7 %></td>
        <td><%= result.a8 %></td>
        <td><%= result.a9 %></td>
        <td><%= result.a10 %></td>
        <td><%= result.a11 %></td>
        <td><%= result.a12 %></td>
        <td><%= result.a13 %></td>
        <td><%= result.a14 %></td>
        <td><%= result.a15 %></td>
        <td><%= result.research %></td>
        <td><%= link_to 'Show', result %></td>
        <td><%= link_to 'Edit', edit_result_path(result) %></td>
        <td><%= link_to 'Destroy', result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Result', new_result_path %>
